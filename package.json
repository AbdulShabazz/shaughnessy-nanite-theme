{
	"name": "shaughnessy-nanite-theme",
	"displayName": "Shaughnessy (Nanite) Theme",
	"description": "A dark theme visual theme for Visual Studio Code",
	"version": "1.0",
	"publisher": "AbdulShabazz",
	"license": "GNU 3.0",
	"author": "Abdul_Shabazz",
	"icon": "logo.png",
	"galleryBanner": {
		"color": "#11131C",
		"theme": "dark"
	},
	"homepage": "https://github.com/AbdulShabazz/shaughnessy-nanite-theme",
	"repository": {
		"type": "git",
		"url": "https://github.com/AbdulShabazz/shaughnessy-nanite-theme.git"
	},
	"bugs": {
		"url": "https://github.com/AbdulShabazz/shaughnessy-nanite-theme/issues"
	},
	"extensionPack": [
		"ShaughnessyNaniteTheme.v001"
	],
	"engines": {
		"node": ">=12",
		"vscode": "^1.51.0"
	},
	"scripts": {
		"build": "run-s cleanup build:ts build:generate-themes build:ui",
		"cleanup": "rimraf build && rimraf dist",
		"lint": "eslint .",
		"build:ui": "node dist/scripts/ui/index.js",
		"build:generate-themes": "node dist/scripts/generator/index.js",
		"build:ts": "tsc -p ./tsconfig.json && ncp dist/src/ build && ncp material-theme.config.json build",
		"postinstall": "node ./node_modules/vscode/bin/install && tsc -p tsconfig.json",
		"release": "git pull && standard-version --release-as minor && git push --follow-tags",
		"release:patch": "git pull && standard-version --release-as patch && git push --follow-tags",
		"release:major": "git pull && standard-version -release-as major && git push --follow-tags"
	},
	"categories": [
		"Themes",
		"Other"
	],
	"keywords": [
		"VSCode",
		"Themes"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"extensionKind": [
		"ui",
		"workspace"
	],
	"main": "./build/material.theme.config",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"contributes": {
		"commands": [
			{
				"command": "materialTheme.setAccent",
				"title": "Set accent color",
				"category": "Material Theme"
			},
			{
				"command": "materialTheme.showReleaseNotes",
				"title": "Release Notes",
				"category": "Material Theme"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Material Theme",
			"properties": {
				"materialTheme.accent": {
					"type": "string",
					"default": "Blue",
					"enum": [
						"Acid Lime",
						"Blue",
						"Breaking Bad",
						"Bright Teal",
						"Cyan",
						"Graphite",
						"Indigo",
						"Lime",
						"Orange",
						"Pink",
						"Purple",
						"Red",
						"Sky",
						"Tomato",
						"Teal",
						"Yellow"
					],
					"description": "Current accent color selected",
					"scope": "window"
				}
			}
		},
		"themes": [
			{
				"label": "Shaughnessy Nanite Theme",
				"path": "./build/Shaughnessy-Nanite.json",
				"uiTheme": "vs-dark"
			}
		]
	},
	"extensionDependency": [
		
	],
	"badges": [
		{
			"url": "",
			"href": "https://github.com/AbdulShabazz/shaughnessy-nanite-theme/issues",
			"description": "Open issues"
		}
	],
	"devDependencies": {
		"@moxer/vscode-theme-generator": "1.21.0",
		"@types/browserify": "12.0.36",
		"@types/fs-extra": "8.1.0",
		"@types/rimraf": "3.0.2",
		"@typescript-eslint/eslint-plugin": "3.3.0",
		"@typescript-eslint/parser": "3.3.0",
		"browserify": "16.5.1",
		"eslint": "7.2.0",
		"eslint-config-xo-space": "0.25.0",
		"eslint-config-xo-typescript": "0.31.0",
		"fs-extra": "9.0.1",
		"ncp": "2.0.0",
		"npm-run-all": "4.1.5",
		"standard-version": "9.5.0",
		"typescript": "4.1.3",
		"vscode": "1.1.37"
	},
	"__metadata": {
		"id": "",
		"publisherDisplayName": "AbdulShabazz",
